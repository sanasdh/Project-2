<%- include("../partials/header") %>
<h1>By Brand & Product</h1>
<div class="grid-container">
  <% productData.forEach(function(b){ %>
  <% newBrand.forEach(function(nB){ %>
  <% newProduct.forEach(function(nP){ %>
  <% if(b.brand==nB && b.product_type== nP){ %>
  <% c++ %>
  <div class="grid-items">
    <img class="search-img" src="<%= b.api_featured_image %>">
    <form action="/products/<%=b._id %>" method="GET" class="search-from">
      <button type="submit">Detail</button>
    </form>
    
  </div>

<% }})})})%>
</div>
<% if(c==0 ){ %>
<h5 class= > No exact result was found</h3>
  <% } %>
